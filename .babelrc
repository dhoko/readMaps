{
  "presets": [
    [
      "env",
      {
          targets: {
              browsers: ['last 2 version', 'ie 11']
          }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-transform-react-jsx", { "pragma": "h" }],
    "babel-plugin-transform-object-rest-spread",
    [
      "babel-plugin-transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}